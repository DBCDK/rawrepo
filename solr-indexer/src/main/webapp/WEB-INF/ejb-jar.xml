<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        version="3.2"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd">
  <enterprise-beans>
    <session>
      <ejb-name>Indexer</ejb-name>
      <env-entry>
        <env-entry-name>solrUrl</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>http://localhost:5015/solr</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>openAgencyUrl</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>workerName</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>solr-sync</env-entry-value>
      </env-entry>
    </session>
    <session>
      <ejb-name>Dispatcher</ejb-name>
      <env-entry>
        <env-entry-name>timeout</env-entry-name>
        <env-entry-type>java.lang.Integer</env-entry-type>
        <env-entry-value>5</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>maxConcurrent</env-entry-name>
        <env-entry-type>java.lang.Integer</env-entry-type>
        <env-entry-value>1</env-entry-value>
      </env-entry>
    </session>
  </enterprise-beans>
</ejb-jar>
